{
  "name": "@monukanyal/multiple-tag",
  "version": "1.0.2",
  "description": "A library for multiple email tag input field",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/monukanyaldev/multiple-tag.git"
  },
  "bugs": {
    "url": "https://github.com/monukanyaldev/multiple-tag/issues"
  },
  "main": "./dist/multipleTag.common.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build-bundle": "vue-cli-service build --target lib --name multipleTag ./src/index.js",
    "lint": "vue-cli-service lint --fix"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "vue": "^2.6.11",
    "vuelidate": "^0.7.5"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.4.0",
    "@vue/cli-plugin-eslint": "~4.4.0",
    "@vue/cli-service": "~5.0.8",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "vue-template-compiler": "^2.6.11",
    "@fortawesome/fontawesome-free": "^5.13.0"
  },
  "files": [
    "dist/*",
    "src/*",
    "public/*",
    "*.json",
    "*.js"
  ],
  "keywords": [
    "javascript",
    "vue",
    "tags",
    "multiple-email-input",
    "multiple-email",
    "vue-component"
  ],
  "homepage": "https://github.com/monukanyaldev/multiple-tag#readme",
  "author": "Monu kanyal",
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
